<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Zepto Login Suite - Run This First" parallel="false" thread-count="1">
    
    <listeners>
        <listener class-name="com.zepto.utilities.ExtentManager"/>
        <listener class-name="com.zepto.utilities.TestListener"/>
    </listeners>
    
    <parameter name="browser" value="chrome"/>
    
    <test name="Login Test - Save Cookies" preserve-order="true">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.zepto.tests.LoginTest">
                <methods>
                    <include name="testLoginWithMobileNumber"/>
                </methods>
            </class>
        </classes>
    </test>
    
</suite>